Create table "objects" (
	"object_type_id" Varchar2 (20) NOT NULL ,
	"object_id" Varchar2 (20) NOT NULL ,
	"name" Varchar2 (20),
	"object_id" Varchar2 (20) NOT NULL ,
primary key ("object_id") 
) 
/

Create table "object_types" (
	"object_type_id" Varchar2 (20) NOT NULL ,
	"object_type_id" Varchar2 (20) NOT NULL ,
	"name" Varchar2 (20),
primary key ("object_type_id") 
) 
/



-- Create Foreign keys section

Alter table "objects" add  foreign key ("object_id") references "objects" ("object_id") 
/

Alter table "objects" add  foreign key ("object_type_id") references "object_types" ("object_type_id") 
/

Alter table "object_types" add  foreign key ("object_type_id") references "object_types" ("object_type_id") 
/
